{
    "name": "proklung/wp.migrations",
    "description": "Adapting Arrilot database migrations for Wordpress.",
    "license": "MIT",
    "keywords": [
        "wordpress",
        "migrations"
    ],
    "authors": [
        {
            "name": "Gavrilov Fedy",
            "email": "fedor.gavrilov.75@mail.ru",
            "homepage": "https://github.com/ProklUng"
        }
    ],
    "support": {
        "issues": "https://github.com/ProklUng/wp.migrations/issues",
        "source": "https://github.com/ProklUng/wp.migrations"
    },
    "autoload": {
        "psr-4": {
            "Arrilot\\BitrixMigrations\\": "src",
            "Arrilot\\BitrixMigrations\\Tests\\": "tests"
        },
        "exclude-from-classmap": [
            "Tests/"
        ]
    },
    "require": {
        "php": ">=7.1 || ^8.0",
        "symfony/console": "^4.4 || ^5.0",
        "illuminate/collections": "^5.0 || ^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "proklung/phpunit-testing-tools": "^1.5",
        "icanhazstring/composer-unused": "^0.7.5"
    },
    "bin": [
        "migrator"
    ]

}
